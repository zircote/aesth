{
  "hooks": [
    {
      "matcher": {
        "event": "session_start"
      },
      "hooks": [
        {
          "type": "command",
          "command": "subcog hook session-start"
        }
      ]
    },
    {
      "matcher": {
        "event": "user_prompt_submit"
      },
      "hooks": [
        {
          "type": "command",
          "command": "sh -c 'subcog hook user-prompt-submit \"$PROMPT\"'"
        }
      ]
    }
  ]
}
